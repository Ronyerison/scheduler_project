<odoo>
  <record id="scheduler_core_procedimento_view_tree" model="ir.ui.view">
    <field name="name">scheduler_core.procedimento.view.tree</field>
    <field name="model">scheduler_core.procedimento</field>
    <field name="type">tree</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="valor"/>
      </tree>
    </field>
  </record>

  <record id="scheduler_core_procedimento_view_form" model="ir.ui.view">
    <field name="name">scheduler_core.procedimento.view.form</field>
    <field name="model">scheduler_core.procedimento</field>
    <field name="type">form</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="valor"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="scheduler_core_procedimento_action" model="ir.actions.act_window">
    <field name="name">Procedimento</field>
    <field name="res_model">scheduler_core.procedimento</field>
    <field name="view_mode">tree,form</field>
    <field name="domain">[]</field>
    <field name="context">{}</field>
  </record>

</odoo>
