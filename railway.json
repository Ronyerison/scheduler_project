{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "railway.Dockerfile"
  },
  "deploy": {
    "startCommand": "odoo -c /etc/odoo/odoo.conf --without-demo=all",
    "healthcheckPath": "/web/login",
    "healthcheckTimeout": 300
  }
}